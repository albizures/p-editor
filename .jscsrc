{
	"fileExtensions": [".js"],
	"esnext": true,
	"preset": "crockford",
	"validateIndentation": "\t",
	"disallowDanglingUnderscores": false,
	"requireCurlyBraces": true,
	"disallowKeywords": ["with"],
	"requireParenthesesAroundIIFE": true,
	"excludeFiles": [
		"dist/**",
		"bundle/**",
		"node_modules/**"
	]
}
