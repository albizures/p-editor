
.panel-actions
	.content-redo, .content-undo
		width: 50%
		padding 5px
		display: inline-block;
		box-sizing: border-box
		button
			width: 100%
			text-align: center
	.content-redo
		padding-left: 0
	.content-lists
		overflow-y scroll
		height calc(100% - 53px)
		.list
			list-style none
			margin 0
			&.undo
				background gray
			&.redo
				background white
