{
  "name": "pixelz",
  "version": "0.1.0",
  "description": "A editor for pixel art",
  "main": "index.js",
  "homepage": "http://pixelzstudio.com",
  "scripts": {
    "server": "nodemon ./index.js",
    "webpack": "webpack-dev-server --open --inline",
    "start": "env-cmd ./.env concurrently \"npm run webpack\" \"npm run server\"",
    "test": "eslint ./src && ava"
  },
  "ava": {
    "files": [
      "test/**/*.js",
      "!test/utils.js"
    ],
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "babel": {
    "plugins": [
      "transform-es2015-destructuring",
      "syntax-object-rest-spread"
    ],
    "presets": [
      "es2015",
      "react"
    ]
  },
  "keywords": [
    "express",
    "webpack",
    "pixel",
    "art"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/albizures/pixelz"
  },
  "author": "Jose Albizures <albizures3601@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.15.3",
    "bluebird": "^3.4.6",
    "body-parser": "^1.15.1",
    "bootstrap-grid": "^2.0.1",
    "classnames": "^2.2.5",
    "co": "^4.6.0",
    "connect-mongo": "^1.3.2",
    "cookie-parser": "^1.4.3",
    "dom-notifications": "^1.1.0",
    "express": "^4.13.3",
    "express-session": "^1.14.2",
    "joi": "^10.0.6",
    "jsonwebtoken": "^7.2.1",
    "lodash": "^4.17.2",
    "mongodb": "^2.2.16",
    "mongoose": "^4.7.3",
    "multer": "^1.2.1",
    "passport": "^0.3.2",
    "passport-twitter": "^1.0.4",
    "pngjs": "^3.0.0",
    "querymen": "^2.1.1",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-dynamic-modal": "^1.1.1",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "react-stonecutter": "^0.3.3",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "serve-favicon": "^2.3.2"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-es2015-destructuring": "^6.19.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "concurrently": "^3.1.0",
    "css-loader": "^0.26.1",
    "env-cmd": "^4.0.0",
    "enzyme": "^2.6.0",
    "eslint": "^3.12.2",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.8.0",
    "eslint-plugin-standard": "^2.0.1",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "image-webpack-loader": "^3.1.0",
    "imports-loader": "^0.7.0",
    "morgan": "^1.7.0",
    "nodemon": "^1.11.0",
    "pug": "^2.0.0-beta6",
    "pug-loader": "^2.3.0",
    "react-addons-test-utils": "^15.4.1",
    "react-dev-utils": "^0.4.2",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.4.0",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2",
    "worker-loader": "^0.7.1"
  }
}
